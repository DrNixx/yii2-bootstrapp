{
    "name": "drnixx/yii2-bootstrapp",
    "description": "Bootstrap5 & Revox.Io Pages widgets for Yii2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Panteleeff",
            "email": "drnix@chess-online.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",

        "yiisoft/yii2": "~2.0.38",
        "yiisoft/yii2-bootstrap5": "~2.0.0",

        "drnixx/yii2-onix": "^1.0",
        "drnixx/yii2-pages-asset": "^3.0"
    },
    "require-dev": {
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4"
    },
    "autoload": {
        "psr-4": {
            "onix\\": "src/"
        }
    },
    "config": {
        "platform": {"php": "7.0"}
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
