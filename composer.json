{
    "name": "drnixx/yii2-bootstrapp",
    "description": "Bootstrap4 & Revox.Io Pages widgets for Yii2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Panteleeff",
            "email": "drnix@chess-online.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "2.1.x-dev",

        "bower-asset/bootstrap": "~4.0.0",
        "bower-asset/popper.js": "~1.12.0",

        "drnixx/yii2-onix": "^1.5.18",
        "drnixx/yii2-pages-asset": "^1.0"
    },
    "require-dev": {
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "codeception/specify": "~0.4.3"
    },
    "autoload": {
        "psr-4": {
            "onix\\": "src/"
        }
    },
    "config": {
        "platform": {"php": "5.6"}
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
